{
  "name": "naver-webtoon-extension",
  "version": "1.0.0",
  "description": "네이버 웹툰 익스텐션.",
  "scripts": {
    "build:chrome": "webpack --env.production --env.browser=chrome",
    "build:whale": "webpack --env.production --env.browser=whale",
    "build": "npm run build:chrome && npm run build:whale",
    "dev:chrome": "webpack --env.browser=chrome",
    "dev:whale": "webpack --env.browser=whale",
    "dev": "npm run dev:chrome && npm run dev:whale",
    "watch:chrome": "webpack -w --env.browser=chrome",
    "watch:whale": "webpack -w --env.browser=whale",
    "lint": "prettier --write ./src/**/*.{ts,tsx}",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^3.9.3",
    "@types/jest": "^24.0.13",
    "@types/naver-whale": "^0.0.0",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/sinon-chrome": "^2.2.6",
    "copy-webpack-plugin": "^5.0.3",
    "coveralls": "^3.0.3",
    "css-loader": "^3.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.0.1",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.7.2",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0",
    "sinon-chrome": "^3.0.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "webpack": "^4.1.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.1",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "date-fns": "^1.30.1",
    "mobx": "^5.11.0",
    "mobx-react": "^6.1.1",
    "react": "^16.8.6",
    "react-contexify": "^4.0.2",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.8",
    "uikit": "^3.1.5"
  }
}
